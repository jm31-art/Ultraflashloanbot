    "run:auto:liq": "arbitrage_env\\Scripts\\python.exe self_discover_hybrid_bot.py --mode liq",
    "run:auto:hybrid": "arbitrage_env\\Scripts\\python.exe self_discover_hybrid_bot.py --mode hybrid",
    
    "run:printer": "python final_printer_2025.py",
    
    "run:ai": "node -e \"console.log('AI-Powered Arbitrage Bot Starting...'); require('./ai/mev_protector').aiMEVProtector.initialize().then(() => console.log('AI Ready!')).catch(console.error);\" && npm run run:printer",
